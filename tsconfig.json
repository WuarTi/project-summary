{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES6",
		"outDir": "out",
		"rootDir": "src",
		"sourceMap": true,
		"strict": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"exclude": ["node_modules", ".vscode-test"]
}
